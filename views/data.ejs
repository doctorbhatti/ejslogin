	<input type="text" placeholder="Enter you academic year." id="myInput">
    <button type="button" onclick="getInputValue();">Academic Year</button>
    
	<div id="meet">
	Class
</div>
<script>
function getInputValue(){
$(document).ready(function(){
    var domain = "docbhatti.tech";
                var inputVal = document.getElementById("myInput").value;
	var options = {
		roomName: inputVal,
		width: calc(100% - 20px),
		height: calc(100% - 20px),
		parentNode: document.querySelector('#meet')
	}
var api = new JitsiMeetExternalAPI(domain, options);
});
}
</script>

